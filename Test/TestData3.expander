let nonrec f =
  \x#0 ->
    \y#1 ->
      cond
        cond
          isInt x#0 ->
            cond
              isInt y#1 -> true#
              true# -> false#
          true# -> false# ->
          let nonrec x = x#0 in
          let nonrec y = y#1 in
          true
        true# ->
          let nonrec x = x#0 in
          let nonrec y = y#1 in
          false